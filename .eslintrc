{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parents": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "no-trailing-spaces": "off",
    "space-before-function-paren": "off",
    "space-before-blocks": "off"
  },
  "extends": ["eslint:recommended", "google"]
}